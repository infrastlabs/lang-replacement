				</replace>
				<replace>
					<target>^\s+KEY AUTH$</target>
					<expect>键值授权</expect>
				</replace>
				<replace>
					<target>^\s+JWT AUTH$</target>
					<expect>JWT授权</expect>
				</replace>
			</item>
		</file>
