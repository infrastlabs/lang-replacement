<?xml version="1.0" encoding="UTF-8" ?>
	<root>
		<file>
			<name>js/app/dashboard/dashboard.html</name>
			<item>
				<replace>
					<target>^\s+Welcome!$</target>
					<expect>欢迎!</expect>
				</replace>
				<replace>
					<target>Make sure your active &lt;a ui-sref=&quot;connections&quot;&gt;connection&lt;/a&gt; is valid and Kong is up and running.</target>
