				</replace>
				<replace>
					<target>^\s+CONNECTIONS$</target>
					<expect>连接端</expect>
				</replace>
				<replace>
					<target>Total Requests: </target>
					<expect>总请求: </expect>
				</replace>
				<replace>
					<target>^\s+CLUSTER \(\{\{clusters.total\}\} nodes\)&amp;</target>
					<expect>集群有 ({{clusters.total}} 节点数)</expect>
				</replace>
			</item>
